{
  "meta": {
    "rule_id": "apt_29_thinktanks_bypass_uac_powershell_fuzz_data",
    "display_name": "APT29 ThinkTanks UAC bypass (powershell) - fuzz skeleton",
    "version": "v1",
    "description": "grammar skeleton extracted from sigma rule; placeholders used"
  },
  "terminals": {
    "executables": [
      {"tok": "powershell.exe", "weight": 1.0},
      {"tok": "<other_exe_placeholder>", "weight": 0.5}
    ],
    "arguments": [
      {"tok": "<arg>", "examples": ["<elevate_arg1>", "<elevate_arg2>"], "weight": 1.0}
    ],
    "wrappers": [
      {"fmt": "{}", "weight": 1.0},
      {"fmt": "powershell -Command {}", "weight": 0.8}
    ],
    "noise": [
      {"tok": "\"\"", "weight": 0.2},
      {"tok": ";", "weight": 0.1}
    ]
  },
  "rules": {
    "payload_core": {
      "structures": [
        [
          {"type":"choose","from":"executables"},
          {"type":"sep","tok":" "},
          {"type":"choose","from":"arguments"}
        ]
      ],
      "constraints": {
        "must_contain_keyword": null,
        "regex_positive": "<COPY_REGEX_FROM_SIGMA_HERE>"
      }
    }
  },
  "mutation_engine": {
    "max_mutations": 3,
    "preserve_keyword": true
  },
  "sampling": {
    "strategy":"epsilon_greedy",
    "epsilon_seed":0.3,
    "epsilon_group":0.15
  }
}
